{
  "name": "client",
  "version": "0.0.0",
  "proxy": "http://localhost:3000",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --poll 100 --disable-host-check --hmr --proxy-config proxy.conf.js --base-href /",
    "start_base": "ng serve --host 0.0.0.0 --poll 500 --disable-host-check",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check --poll 500 --proxy-config proxy.conf.json --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:prod": "ng build --configuration production --output-path=dist",
    "build:ssr": "ng build --configuration production && ng run frontend:server:production",
    "build:output": "npm run build:ssr --output-path=dist",
    "prerender": "ng run frontend:prerender",
    "dev-server": "concurrently \"nodemon ../server/index.js --watch server\" \"npm run serve\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.6",
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/forms": "~12.2.6",
    "@angular/platform-browser": "~12.2.6",
    "@angular/platform-browser-dynamic": "~12.2.6",
    "@angular/platform-server": "~12.2.6",
    "@angular/router": "~12.2.6",
    "@nguniversal/express-engine": "^12.0.0",
    "font-awesome": "^4.7.0",
    "chart.js": "^2.9.3",
    "materialize-css": "^1.0.0-rc.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "^12.2.6",
    "@angular/compiler-cli": "~12.2.6",
    "@angular/language-service": "^12.2.6",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^15.6.1",
    "codelyzer": "^6.0.2",
    "concurrently": "6.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
